import requests
from bs4 import BeautifulSoup

url="https://drive.carrefour.be/nl/Bier/c/BEDR307000000000000?q=%3Aname-asc&page=0"

resp=requests.get(url)

soup=BeautifulSoup(resp.content,"lxml")
product_grid=soup.find_all("div", {"class":"product__listing product__grid"})
ulrs=product_grid[0].find_all("a")
for items in ulrs:
    print(f"items['href'])
